@article{Tunka2020,
title = "The primary cosmic-ray energy spectrum measured with the Tunka-133 array",
journal = "Astroparticle Physics",
volume = "117",
pages = "102406",
year = "2020",
issn = "0927-6505",
doi = "10.1016/j.astropartphys.2019.102406",
author = "N.M. Budnev and A. Chiavassa and O.A. Gress and T.I. Gress and A.N. Dyachok and N.I. Karpov and N.N. Kalmykov and E.E. Korosteleva and V.A. Kozhin and L.A. Kuzmichev and B.K. Lubsandorzhiev and N.B. Lubsandorzhiev and R.R. Mirgazov and E.A. Osipova and M.I. Panasyuk and L.V. Pankov and E.G. Popova and V.V. Prosin and V.S. Ptuskin and Yu.A. Semeney and A.A. Silaev and A.A. Silaev(junior) and A.V. Skurikhin and C. Spiering and L.G. Sveshnikova and A.V. Zagorodnikov",
keywords = "High energy cosmic rays, air shower, Cherenkov radiation, Energy spectrum",
abstract = "The EAS Cherenkov light array Tunka-133, with ~ 3 km2 geometric area, is taking data since 2009.The array permits a detailed study of energy spectrum and mass composition of cosmic rays in the energy range from 6·1015 to 1018~eV. We describe the methods of time and amplitude calibration of the array and the methods of EAS parameters reconstruction. We present the all-particle energy spectrum, based on 7 seasons of operation."
}


